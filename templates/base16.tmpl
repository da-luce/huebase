scheme: "{{ .Metadata.Name }}"
author: "{{ .Metadata.Author }}"
base00: "{{ or .SpecialColors.Background.Hex        .AnsiColors.Black.Hex }}"       # Default Background
base01: "{{ or .SpecialColors.BackgroundLight.Hex   .AnsiColors.BrightBlack.Hex }}" # Lighter Background (Used for status bars, line number and folding marks)
base02: "{{ .SpecialColors.Selection.Hex }}"        # Selection Background
base03: "{{ .ScopeColors.Basic.Comment.Hex }}"      # Comments, Invisibles, Line Highlighting
base04: "{{ .SpecialColors.ForegroundBright.Hex }}" # Dark Foreground (Used for status bars)
base05: "{{ .SpecialColors.Foreground.Hex }}"       # Default Foreground, Caret, Delimiters, Operators
base06: "{{ .AnsiColors.White.Hex }}"               # Light Foreground (Not often used)
base07: "{{ .AnsiColors.BrightWhite.Hex }}"         # Light Background (Not often used)
base08: "{{ .ScopeColors.Basic.Variable.Hex }}"     # Variables, XML Tags, Markup Link Text, Markup Lists, Diff Deleted
base09: "{{ .ScopeColors.Basic.Constant.Hex }}"     # Integers, Boolean, Constants, XML Attributes, Markup Link Url
base0A: "{{ .ScopeColors.Advanced.Class.Hex }}"     # Classes, Markup Bold, Search Text Background
base0B: "{{ .ScopeColors.Basic.String.Hex }}"       # Strings, Inherited Class, Markup Code, Diff Inserted
base0C: "{{ .Miscellaneous.Regex.Hex }}"            # Support, Regular Expressions, Escape Characters, Markup Quotes
base0D: "{{ .ScopeColors.Basic.Function.Hex }}"     # Functions, Methods, Attribute IDs, Headings
base0E: "{{ .ScopeColors.Basic.Keyword.Hex }}"      # Keywords, Storage, Selector, Markup Italic, Diff Changed
base0F: "{{ .ScopeColors.Diagnostics.Deprecated.Hex }}" # Deprecated, Opening/Closing Embedded Language Tags, e.g. <?php ?>
